jQuery(function () {

    var versionForum = 'ModernBB',
        $chemin,
        $eltParent = '.postdetails poster-profile';

    $chemin = ".post .post_details .post_profile .encava .infos-posteur";

      
    $( $chemin ).each(function( index ) {

        var champs = $(this).find('.label').html() ;
 
        if( champs == "PSEUDO :" || champs == "PSEUDO" || champs == "PSEUDO : " || champs == "Cr√©dits : " || champs == "Faceclaim : " || champs == "Multicomptes : "){
            $(this).parents($eltParent).find('.other').append(this);
 
        }
 
    });
});
